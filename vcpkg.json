{
	"$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
	"name": "srb2",
	"version": "1.0.0",
	"dependencies": [
		{
			"name": "zlib"
		},
		{
			"name": "libpng"
		},
		{
			"name": "curl",
			"platform": "!(osx & !native)"
		},
		{
			"name": "libopenmpt"
		},
		{
			"name": "libgme"
		},
		{
			"name": "miniupnpc"
		},
		{
			"name": "sdl2",
			"default-features": false,
			"features": [
				{
					"name": "wayland",
					"platform": "linux"
				},
				{
					"name": "x11",
					"platform": "!windows"
				}
			]
		},
		{
			"name": "sdl2-mixer-ext",
			"default-features": true,
			"features": [
				{
					"name": "ffmpeg"
				},
				{
					"name": "fluidsynth",
					"platform": "!(osx & !native)"
				},
				{
					"name": "libflac"
				},
				{
					"name": "libgme"
				},
				{
					"name": "libmodplug"
				},
				{
					"name": "libvorbis"
				},
				{
					"name": "libxmp"
				},
				{
					"name": "mpg123"
				},
				{
					"name": "opusfile"
				},
				{
					"name": "pxtone"
				},
				{
					"name": "timidity"
				}
			]
		}
	]
}
